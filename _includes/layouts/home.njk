
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/newhome.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    {% if css %}
      {% for link in css %}
        <link rel="stylesheet" href="{{ link | url }}">
      {% endfor %}
    {% endif %}
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="icon" href="/favicon.ico" sizes="32x32">
    {% for size in [152, 167, 180] %}
      <link rel="apple-touch-icon" type="image/png" sizes="{{ size }}x{{ size }}" href="favicon-{{ size }}x{{ size }}.png">
    {% endfor %}
    <meta property="og:title" content="{{ renderData.title or title or metadata.title}}">
    <meta property="og:description" content="{{ renderData.description or description or metadata.description}}">
    <meta property="og:image" content="/img/bita-banner.jpg">
    <meta property="og:url" content="http://better-in-the-app.github.io">
    <meta name="theme-color" content="#333" media="(prefers-color-scheme: light)">
    <meta name="theme-color" content="#333" media="(prefers-color-scheme: dark)">
  </head>
  <body class="new-body">
    <header>
      <nav>
        <ul class="nav">
        {%- for entry in collections.all | eleventyNavigation %}
          {% if entry.url != page.url %}
          <li class="nav-item"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
          {% endif %}
        {%- endfor %}
        </ul>
      </nav>
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

    <footer></footer>

    {# Current page: {{ page.url | url }} --> #}
  </body>
</html>
